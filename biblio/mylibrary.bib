}

@article{Martinezengagementmedecinsgeneralistes2016,
  title = {L’engagement des médecins généralistes français dans la vaccination : l’étude DIVA (Déterminants des Intentions de Vaccination)},
  shorttitle = {L’engagement des médecins généralistes français dans la vaccination},
  author = {Martinez, Luc and Tugaut, Béatrice and Raineri, François and Arnould, Benoit and Seyler, Didier and Arnould, Pascale and Benmedjahed, Khadra and Coindard, Guillaume and Denis, François and Gallais, Jean-Luc and Duhot, Didier},
  date = {2016-04-22},
  journaltitle = {Sante Publique},
  volume = {Vol. 28},
  number = {1},
  pages = {19--32},
  publisher = {{S.F.S.P.}},
  issn = {0995-3914},
  url = {https://www-cairn-info.passerelle.univ-rennes1.fr/revue-sante-publique-2016-1-page-19.htm},
  urldate = {2021-07-05},
  abstract = {Objectifs\&\#160;: La vaccination est efficace pour r\&\#233;duire morbidit\&\#233; et mortalit\&\#233; li\&\#233;es aux maladies infectieuses. Les m\&\#233;decins g\&\#233;n\&\#233;ralistes sont le premier effecteur de la vaccination en France. Notre objectif \&\#233;tait de mener une \&\#233;tude exploratoire qualitative chez les m\&\#233;decins g\&\#233;n\&\#233;ralistes afin de caract\&\#233;riser les facteurs associ\&\#233;s \&\#224; leur engagement dans la vaccination. M\&\#233;thodes\&\#160;: Une recherche qualitative par entretiens de groupe suivant un guide de discussion semi-directif a \&\#233;t\&\#233; r\&\#233;alis\&\#233;e. Trente-six m\&\#233;decins g\&\#233;n\&\#233;ralistes de diverses r\&\#233;gions fran\&\#231;aises ont particip\&\#233; \&\#224; six entretiens de groupe. Une analyse qualitative par codage des transcriptions a \&\#233;t\&\#233; men\&\#233;e afin d\&\#8217;identifier les facteurs influen\&\#231;ant l\&\#8217;attitude de prescription de la vaccination des m\&\#233;decins g\&\#233;n\&\#233;ralistes. Ces facteurs ont ensuite \&\#233;t\&\#233; regroup\&\#233;s en th\&\#232;mes. La saturation a \&\#233;t\&\#233; \&\#233;valu\&\#233;e. R\&\#233;sultats\&\#160;: Les vaccinations relatives \&\#224; la dipht\&\#233;rie, t\&\#233;tanos, poliomy\&\#233;lite, rougeole, oreillons, rub\&\#233;ole, h\&\#233;patite B, tuberculose, infections \&\#224; pneumocoque, m\&\#233;ningocoque, papillomavirus humains, rotavirus, coqueluche, varicelle, grippe ont \&\#233;t\&\#233; abord\&\#233;es lors de tous les entretiens de groupe. La saturation a \&\#233;t\&\#233; atteinte d\&\#232;s le quatri\&\#232;me entretien de groupe. Les 40 facteurs identifi\&\#233;s se r\&\#233;partissaient en six th\&\#232;mes\&\#160;: vaccin, maladie, v\&\#233;cu des m\&\#233;decins g\&\#233;n\&\#233;ralistes, aspects pratiques, b\&\#233;n\&\#233;fices attendus et relation m\&\#233;decin g\&\#233;n\&\#233;raliste-patient. Conclusions\&\#160;: Cette \&\#233;tude a permis d\&\#8217;identifier les facteurs comportementaux et organisationnels qui influencent l\&\#8217;attitude des m\&\#233;decins g\&\#233;n\&\#233;ralistes vis-\&\#224;-vis de la vaccination. Ces attitudes et ces facteurs \&\#233;taient variables selon les maladies et les vaccins. Les th\&\#232;mes et facteurs identifi\&\#233;s ont servi de base au d\&\#233;veloppement d\&\#8217;un questionnaire \&\#233;valuant les D\&\#233;terminants des Intentions de Vaccination (DIVA) des m\&\#233;decins g\&\#233;n\&\#233;ralistes.},
  langid = {french},
