@article{SmyrnaiosactualiteselonGoogle2009,
  title = {L’actualité selon Google L’emprise du principal moteur de recherche sur l’information en ligne},
  author = {Smyrnaios, Nikos and Rebillard, Franck},
  date = {2009},
  journaltitle = {Communication langages},
  volume = {N° 160},
