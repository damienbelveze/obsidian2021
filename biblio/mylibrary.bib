@article{HasselbalchCognitivedeficitsremitted2012,
  title = {Cognitive Deficits in the Remitted State of Unipolar Depressive Disorder.},
  author = {Hasselbalch, Bo Jacob and Knorr, Ulla and Hasselbalch, Steen Gregers and Gade, Anders and Kessing, Lars Vedel},
  date = {2012-09},
  journaltitle = {Neuropsychology},
