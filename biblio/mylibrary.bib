@article{PlutzerTeachingevolutionpublic2020,
  title = {Teaching Evolution in {{U}}.{{S}}. Public Schools: A Continuing Challenge},
  shorttitle = {Teaching Evolution in {{U}}.{{S}}. Public Schools},
  author = {Plutzer, Eric and Branch, Glenn and Reid, Ann},
  date = {2020-06-09},
  journaltitle = {Evolution: Education and Outreach},
  shortjournal = {Evolution: Education and Outreach},
  volume = {13},
  number = {1},
  pages = {14},
  issn = {1936-6434},
  doi = {10.1186/s12052-020-00126-8},
  url = {https://doi.org/10.1186/s12052-020-00126-8},
