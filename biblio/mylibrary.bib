@article{WierengaLupusSilicaDietary2019,
  title = {Lupus, {{Silica}}, and {{Dietary Omega}}-3 {{Fatty Acid Interventions}}},
  author = {Wierenga, K.A. and Harkema, J.R. and Pestka, J.J.},
  date = {2019},
  journaltitle = {Toxicologic Pathology},
  shortjournal = {Toxicol. Pathol.},
  volume = {47},
  number = {8},
  pages = {1004--1011},
  issn = {1533-1601},
  doi = {10.1177/0192623319878398},
  url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2003717396&from=export},
  abstract = {Two environmental factors, crystalline silica (cSiO2), a toxic airborne particle encountered occupationally, and docosahexaenoic acid (DHA), a dietary omega-3 highly unsaturated fatty acid (HUFA), have the potential to influence the development of systemic lupus erythematosus (lupus). Using the NZBWF1 mouse, which spontaneously develops lupus, we found that intranasal exposure to cSiO2 significantly decreases latency and promotes rapid progression of the disease. Specifically, cSiO2 induces the development of ectopic lymphoid structures (ELS) containing germinal centers in the lungs that yield vigorous and diverse autoantibody responses locally and systemically. Transcriptomic analysis revealed that cSiO2 promotes a robust type I interferon gene signature that likely precipitates ELS neogenesis. Intriguingly, dietary supplementation with human-relevant doses of DHA impedes cSiO2-induced gene expression, ELS neogenesis, autoantibody elevation, and glomerulonephritis in this lupus-prone mouse model. Together, our findings point to the feasibility of enhancing tissue omega-3 HUFAs as a personalized nutritional intervention to impede onset and progression of environment-triggered autoimmune disease.},
  langid = {english},
  keywords = {antibody response,article,autoantibody,autoimmune disease,diet supplementation,disease course,docosahexaenoic acid,drug effect,environmental factor,exposure,feasibility study,gene expression,germinal center,glomerulonephritis,inflammation,interferon,lung,lupus erythematosus nephritis,nonhuman,NZBxNZW F1 mouse,omega 3 fatty acid,personalized nutrition,priority journal,silicon dioxide,systemic lupus erythematosus,tertiary lymphoid structure,transcriptomics,unsaturated fatty acid},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Toxicologic Pathology2019\\Wierenga_Harkema_Pestka_2019_Lupus, Silica, and Dietary Omega-3 Fatty Acid Interventions_.pdf}
}

