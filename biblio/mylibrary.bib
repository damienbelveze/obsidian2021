@article{BraaschPromotingsecondaryschool2013,
  title = {Promoting Secondary School Students’ Evaluation of Source Features of Multiple Documents},
  author = {Braasch, Jason L.G. and Bråten, Ivar and Strømsø, Helge I. and Anmarkrud, Øistein and Ferguson, Leila E.},
  date = {2013-07},
  journaltitle = {Contemporary Educational Psychology},
