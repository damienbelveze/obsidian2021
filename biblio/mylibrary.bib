@article{YangArmingpublicartificial2019,
  title = {Arming the Public with Artificial Intelligence to Counter Social Bots},
  author = {Yang, Kai-Cheng and Varol, Onur and Davis, Clayton A. and Ferrara, Emilio and Flammini, Alessandro and Menczer, Filippo},
  date = {2019-02-06},
  journaltitle = {Human Behavior and Emerging Technologies},
  eprint = {1901.00912},
  eprinttype = {arxiv},
  pages = {e115},
  issn = {2578-1863, 2578-1863},
  doi = {10.1002/hbe2.115},
  url = {http://arxiv.org/abs/1901.00912},
  urldate = {2019-02-15},
  abstract = {The increased relevance of social media in our daily life has been accompanied by efforts to manipulate online conversations and opinions. Deceptive social bots -- automated or semi-automated accounts designed to impersonate humans -- have been successfully exploited for these kinds of abuse. Researchers have responded by developing AI tools to arm the public in the fight against social bots. Here we review the literature on different types of bots, their impact, and detection methods. We use the case study of Botometer, a popular bot detection tool developed at Indiana University, to illustrate how people interact with AI countermeasures. A user experience survey suggests that bot detection has become an integral part of the social media experience for many users. However, barriers in interpreting the output of AI tools can lead to fundamental misunderstandings. The arms race between machine learning methods to develop sophisticated bots and effective countermeasures makes it necessary to update the training data and features of detection tools. We again use the Botometer case to illustrate both algorithmic and interpretability improvements of bot scores, designed to meet user expectations. We conclude by discussing how future AI developments may affect the fight between malicious bots and the public.},
  archiveprefix = {arXiv},
  annotation = {ZSCC: 0000105},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Human Behavior and Emerging Technologies2019\\Yang_Varol & al._2019_Arming the public with artificial intelligence to counter social bots_.pdf;D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\storage\\WJAHSZRT\\1901.html}
}

@article{YaoTranscriptomeanalysisreveals2020,
  title = {Transcriptome Analysis Reveals a Protective Role of Liver {{X}} Receptor Alpha against Silica Particle-Induced Experimental Silicosis},
  author = {Yao, W. and Yang, P. and Qi, Y. and Jin, L. and Zhao, A. and Ding, M. and Wang, D. and Li, Y. and Hao, C.},
  date = {2020},
  journaltitle = {Science of the Total Environment},
  shortjournal = {Sci. Total Environ.},
  volume = {747},
  issn = {1879-1026},
  doi = {10.1016/j.scitotenv.2020.141531},
  url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2007407423&from=export},
  abstract = {Silicosis, a severe and irreversible form of pulmonary fibrosis (PF) caused by long-term exposure to dust particles in production environments, is the biggest occupational health concern in China and most low-income countries. The transdifferentiation of pulmonary fibroblasts is the terminal event in silicosis, and specific transcription factors (TFs) play a crucial role in this condition. However, the relationship between TF-mediated regulation and silicosis remains unknown. We performed a transcriptomic analysis to elucidate this relationship, and our results revealed that two TFs, EGR2 and BHLHE40, were upregulated and five, i.e., TBX2, NR1H3 (LXRα), NR2F1, PPARG (PPARγ), and EPAS1, were downregulated in activated fibroblasts. Notably, PPARγ and LXRα expression was also decreased in an experimental mouse model of silicosis. The mechanism underlying these changes may involve TGF-β1 secretion from silica-exposed alveolar macrophages, causing PPARγ and LXRα downregulation, which in turn would result in aberrant α-SMA transcription. Our results suggest that LXRα is a potential target for the prevention of silicosis and PF.},
  issue = {(Yao W.; Yang P.; Qi Y.; Jin L.; Zhao A.; Ding M.; Wang D.; Li Y.; Hao C., haochangfu@126.com) Department of Occupational and Environmental Health, School of Public Health, Zhengzhou University, Henan, China},
  langid = {english},
  keywords = {alpha smooth muscle actin,animal experiment,animal model,animal tissue,article,C57BL/6N mouse,controlled study,down regulation,early growth response factor 2,liver X receptor alpha,macrophage,male,mouse,nonhuman,particle size,peroxisome proliferator activated receptor gamma,phagocytosis,priority journal,protein expression,signal transduction,silica nanoparticle,silicosis,transcription factor,transcription factor BHLHE4,transcription factor EPAS1,transcription factor NR1H3,transcription factor NR2F1,transcription factor TBX2,transcriptome,transcriptomics,transforming growth factor beta1,unclassified drug,upregulation},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Science of the Total Environment2020\\Yao_Yang & al._2020_Transcriptome analysis reveals a protective role of liver X receptor alpha_.pdf}
}

