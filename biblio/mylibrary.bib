@article{GroolLocationcerebrovasculardegenerative2011,
  title = {Location of Cerebrovascular and Degenerative Changes, Depressive Symptoms and Cognitive Functioning in Later Life: The {{SMART-Medea}} Study.},
  author = {Grool, Anne M. and van der Graaf, Yolanda and Mali, Willem P. T. M. and Geerlings, Mirjam I.},
