@inproceedings{dechandEncryptionWeDon2019,
  title = {In {{Encryption We Don}}‚Äôt {{Trust}}: {{The Effect}} of {{End-to-End Encryption}} to the {{Masses}} on {{User Perception}}},
  shorttitle = {In {{Encryption We Don}}‚Äôt {{Trust}}},
  booktitle = {2019 {{IEEE European Symposium}} on {{Security}} and {{Privacy}} ({{EuroS}}\&{{P}})},
  author = {Dechand, Sergej and Naiakshina, Alena and Danilova, Anastasia and Smith, Matthew},
