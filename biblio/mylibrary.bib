  booktitle = {Proceedings of the 2017 {{IEEE}}/{{ACM International Conference}} on {{Advances}} in {{Social Networks Analysis}} and {{Mining}} 2017},
  author = {Jia, Haofeng and Saule, Erik},
  date = {2017-07-31},
