  abstract = {OBJECTIVE: To compare patterns of cognitive performance in older adults with late-onset depression (LOD;~≥65 years of age) with that of older adults with  early-onset depression (EOD;~{$<$}65 years). METHODS: Participants were 171 adults aged  60 years or older with major depression and executive dysfunction who were  participating in a randomized psychotherapy trial. Participants included 72 LOD and  99 EOD individuals. Cognitive performance on measures of verbal learning, memory,  and executive functioning were evaluated. Demographic and clinical characteristics,  severity of cerebrovascular risk factors, and disability ratings were also compared  between groups. RESULTS: The LOD group was older and had fewer previous episodes of  depression and lower severity of depression compared with EOD participants. The LOD  group demonstrated poorer performance on measures of verbal learning (F(1,161)~=  4.28, p~= 0.04) and memory (F(1,160)~= 4.65, p~= 0.03) than the EOD group. Linear  regression analysis demonstrated that LOD and fewer years of education were  significant predictors of poorer verbal learning (F(7,114)~= 6.25, p~{$<$}0.001) and  memory (F(7,113)=7.24, p~{$<$}0.001). Performance on measures of executive functioning,  severity of vascular risk factors, and disability ratings did not differ between the  two groups. CONCLUSION: In older adults with depression and executive dysfunction,  LOD was associated with poorer performance on measures of verbal learning and  memory. Aging-related brain changes associated with LOD may play a more important  role, leading to dysfunction in these cognitive domains than a history of recurrent  depressive episodes in older adults with a dysexecutive syndrome.},
  langid = {english},
  pmcid = {PMC4145037},
  keywords = {*Age of Onset,age at onset,Age Factors,Aged,Aged; 80 and over,Cognition Disorders/epidemiology/*physiopathology,Cognitive functioning,Depressive Disorder; Major/epidemiology/*physiopathology,disability,executive dysfunction,Executive Function/*physiology,Female,Humans,late onset,late-life depression,learning,Male,memory,Middle Aged,Randomized Controlled Trials as Topic,vascular risk factors}
}

@article{MackinCognitiveoutcomespsychotherapeutic2014,
  title = {Cognitive Outcomes after Psychotherapeutic Interventions for Major Depression in Older Adults with Executive Dysfunction.},
  author = {Mackin, R. Scott and Nelson, J. Craig and Delucchi, Kevin and Raue, Patrick and Byers, Amy and Barnes, Deborah and Satre, Derek D. and Yaffe, Kristine and Alexopoulos, George S. and Arean, Patricia A.},
  date = {2014-12},
  journaltitle = {The American journal of geriatric psychiatry : official journal of the American Association for Geriatric Psychiatry},
  shortjournal = {Am J Geriatr Psychiatry},
  volume = {22},
  number = {12},
  eprint = {24378255},
  eprinttype = {pmid},
  pages = {1496--1503},
  issn = {1545-7214 1064-7481},
  doi = {10/f6p37p},
  abstract = {OBJECTIVE: The purpose of this study was to determine the impact of psychotherapy on cognitive functioning in older adults with late-life depression (LLD) and executive  dysfunction. METHODS: Two hundred twenty-one adults aged 60 years and older  participated in a randomized clinical trial comparing the efficacy of Problem  Solving Therapy (PST) and Supportive Therapy (ST) for LLD. Cognitive performance on  seven tests of executive functioning, verbal learning, and memory was evaluated at  baseline, after 12 weeks of treatment, and at 24 weeks after the completion of  treatment. RESULTS: Performance on a measure of executive functioning with a  significant information processing speed component (Stroop Color and Word Test)  improved after treatment, F (1, 312)~= 8.50, p~= 0.002, and improved performance was  associated with a reduction in depressive symptoms but not treatment type.  Performance on other measures of executive functioning, verbal learning, and memory  did not change significantly after 12 weeks of psychotherapy treatment. CONCLUSION:  Our results suggest that improvements in cognitive functioning after psychotherapy  treatment for depression in older adults with executive dysfunction are likely focal  and not distributed across all cognitive domains. Although previous analyses  reported that PST was superior to ST in the treatment of depression, this analysis  indicated no difference between the two treatments with regard to improvements in  cognitive functioning.},
  langid = {english},
  pmcid = {PMC4108572},
  keywords = {Aged,Aged; 80 and over,Cognition Disorders/diagnosis/epidemiology/*therapy,Cognitive functioning,Comorbidity,Depressive Disorder; Major/epidemiology/*therapy,executive dysfunction,Executive Function/*physiology,Female,Humans,information processing speed,late-life depression,learning,Male,memory,Middle Aged,mild cognitive impairment,psychotherapy,Psychotherapy/*methods,Severity of Illness Index,Treatment Outcome}
}

