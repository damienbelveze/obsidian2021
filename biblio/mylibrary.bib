@article{HawiDNAVariationSNAP252013,
  title = {{{DNA Variation}} in the {{SNAP25 Gene Confers Risk}} to {{ADHD}} and {{Is Associated}} with {{Reduced Expression}} in {{Prefrontal Cortex}}},
  author = {Hawi, Ziarih and Matthews, Natasha and Wagner, Joseph and Wallace, Robyn H. and Butler, Tim J. and Vance, Alasdair and Kent, Lindsey and Gill, Michael and Bellgrove, Mark A.},
  date = {2013-04-12},
  journaltitle = {Plos One},
