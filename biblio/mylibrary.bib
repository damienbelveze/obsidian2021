  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Clinical Pediatrics2016\\Batada_Jacobson_2016_Prevalence of Artificial Food Colors in Grocery Store Products Marketed to_.pdf}
