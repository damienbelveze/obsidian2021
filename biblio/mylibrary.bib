@article{TeixeiradaSilvaSignsdivisivenessdiscrimination2021,
  title = {Signs of Divisiveness, Discrimination and Stigmatization Caused by {{Jeffrey Beall}}'s ‚ÄúPredatory‚Äù Open Access Publishing Blacklists and Philosophy},
