@article{Gunningdigitalinterventiontargeting2021a,
  title = {A Digital Intervention Targeting Cognitive Control Network Dysfunction in Middle Age and Older Adults with Major Depression.},
  author = {Gunning, Faith M. and Anguera, Joaquin A. and Victoria, Lindsay W. and Are√°n, Patricia A.},
