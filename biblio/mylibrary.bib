@article{Beenecallactionlibrarians2021,
  title = {A Call to Action for Librarians: {{Countering}} Conspiracy Theories in the Age of {{QAnon}}},
  shorttitle = {A Call to Action for Librarians},
  author = {Beene, Stephanie and Greer, Katie},
  date = {2021-01-01},
  journaltitle = {The Journal of Academic Librarianship},
