@article{AliEarlyEggProduction1985,
  title = {Early {{Egg Production}} in {{Genetically Blind}} (Rc/Rc) {{Chickens}} in {{Comparison}} with {{Sighted}} ({{Rc}}+/Rc) {{Controls}}},
  author = {Ali, AHMED and Cheng, KIMBERLY M.},
  date = {1985-05-01},
