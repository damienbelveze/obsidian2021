@article{BronnerCequInternet2011,
  title = {Ce qu’Internet fait à la diffusion des croyances},
  author = {Bronner, Gérald},
  date = {2011-10-15},
  journaltitle = {Revue européenne des sciences sociales. European Journal of Social Sciences},
  number = {49-1},
