@article{HenskensEffectsPhysicalActivity2018,
  title = {Effects of {{Physical Activity}} in {{Nursing Home Residents}} with {{Dementia}}: {{A Randomized Controlled Trial}}.},
  author = {Henskens, Marinda and Nauta, Ilse M. and van Eekeren, Marieke C. A. and Scherder, Erik J. A.},
  options = {useprefix=true},
  date = {2018},
  journaltitle = {Dementia and geriatric cognitive disorders},
  shortjournal = {Dement Geriatr Cogn Disord},
  volume = {46},
  number = {1-2},
  eprint = {30145595},
  eprinttype = {pmid},
  pages = {60--80},
  issn = {1421-9824 1420-8008},
  doi = {10/gd4z3d},
  abstract = {BACKGROUND/AIMS: There is no consensus regarding the optimal nonpharmacological intervention to slow down dementia-related decline. We examined whether physical  stimulation interventions were effective in reducing cognitive, physical, mood, and  behavioral decline in nursing home residents with dementia. METHODS: Eighty-seven  nursing home residents with dementia were randomly assigned to 3 physical activity  interventions: activities of daily living (ADL) training, multicomponent exercise  training, or combined multicomponent exercise and ADL training. Outcomes were  measured at baseline, and after 3 and 6 months. RESULTS: A 6-month ADL training  benefitted executive functions, physical endurance, and depression among men.  Exercise training benefitted only grip strength of participants with  mild-to-moderate cognitive impairment. A combined training benefitted functional  mobility compared to ADL training, depressive symptoms and agitation compared to  exercise training, and physical endurance compared to no physical stimulation.  CONCLUSIONS: ADL training appears to be effective for nursing home residents with  moderately severe dementia. It remains unclear whether exercise training is an  effective type of stimulation.},
  langid = {english},
  pmcid = {PMC6187838},
  keywords = {*Activities of daily living,*Activities of Daily Living/psychology,*Behavior,*Cognition,*Dementia,*Exercise,*Exercise/psychology,*Homes for the Aged,*Mood,*Nursing home,*Nursing Homes,*Physical functions,Aged,Aged; 80 and over,Alzheimer Disease/diagnosis/psychology/*therapy,Combined Modality Therapy,Depressive Disorder/diagnosis/psychology/therapy,Executive Function,Female,Hand Strength,Humans,Male,Physical Endurance,Psychomotor Agitation/psychology/therapy,Sex Factors}
