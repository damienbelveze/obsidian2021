@article{RovinaReviewExtractionAnalytical2017,
  title = {A {{Review}} of {{Extraction}} and {{Analytical Methods}} for the {{Determination}} of {{Tartrazine}} ({{E}} 102) in {{Foodstuffs}}},
  author = {Rovina, Kobun and Siddiquee, Shafiquzzaman and Shaarani, Sharifudin Md},
  date = {2017},
  journaltitle = {Critical Reviews in Analytical Chemistry},
  shortjournal = {Crit. Rev. Anal. Chem.},
