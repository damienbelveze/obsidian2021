@article{MichelotEspritcritiqueestu2020,
  title = {« {{Esprit}} (Critique), Es-Tu Là? » {{Enseigner}} Aux Compétences Numériques et Informationnelles, Un Enjeu Sociétal},
  shorttitle = {« {{Esprit}} (Critique), Es-Tu Là?},
  author = {Michelot, Florent},
  date = {2020-10-09},
  journaltitle = {Revue internationale des technologies en pédagogie universitaire},
  shortjournal = {Revue internationale des technologies en pédagogie universitaire},
  volume = {17},
  pages = {96--103},
  doi = {10/ghd73v},
  abstract = {Dans cet article, nous rappelons les nombreux défis créés par le Web, notamment quant à la multiplication des infox. Contrairement à ce que l’on a pu croire, l’accès au Web n’est pas nécessairement vertueux et les jeunes générations (les digital natives) ne sont pas les expert(e)s que l’on imagine parfois. Il était donc indispensable de réinventer la façon dont les référentiels de compétences abordent le numérique. L’UNESCO ou la Commission européenne ont pris cette voie, tout comme le Québec, et ont décidé d’associer des compétences de haut niveau (ex. la pensée critique, la culture de l’information) à la compétence numérique, pour que celle-ci ne se limite pas à des habiletés instrumentales telles qu’allumer un ordinateur, ouvrir son fureteur et se rendre sur un moteur de recherche. La compétence numérique est en fait une nécessité sociétale dont il faut prendre la mesure pour ne pas créer de nouvelles fractures. Quelques situations pédagogiques au postsecondaire permettent d’illustrer la façon dont la compétence numérique peut être abordée dans différentes matières.},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Revue internationale des technologies en pédagogie universitaire2020\\Michelot_2020_« Esprit (critique), es-tu là_.pdf}
}

