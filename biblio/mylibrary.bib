@report{PennycookFightingCOVID19misinformation2020,
  type = {preprint},
  title = {Fighting {{COVID-19}} Misinformation on Social Media: {{Experimental}} Evidence for a Scalable Accuracy Nudge Intervention},
  shorttitle = {Fighting {{COVID-19}} Misinformation on Social Media},
  author = {Pennycook, Gordon and McPhetres, Jonathon and Zhang, Yunhao and Rand, David Gertler},
  date = {2020-03-17},
  institution = {{PsyArXiv}},
  url = {https://osf.io/uhbk9},
  urldate = {2020-05-05},
  abstract = {Misinformation can amplify humanity's greatest challenges. A salient recent example of this is the COVID-19 pandemic, which has bred a multitude of falsehoods even as truth has increasingly become a matter of life-and-death. Here we investigate why people believe and spread false (and true) news content about COVID-19, and test an intervention intended to increase the truthfulness of the content people share on social media. Across two studies with over 1,600 participants (quota-matched to the American public on age, gender, ethnicity and geographic region), we find support for the idea that people share false claims about COVID-19 in part because they simply fail to think sufficiently about whether or not content is accurate when deciding what to share. In Study 1, participants were far worse at discerning between true and false content when deciding what they would share on social media relative to when they are asked directly about accuracy. Furthermore, participants who engaged in more analytic thinking and had greater science knowledge were more discerning in their belief and sharing. In Study 2, we found that a simple accuracy reminder at the beginning of the study ‚Äì i.e., asking people to judge the accuracy of a non-COVID-19-related headline ‚Äì more than doubled the level of truth discernment in participants‚Äô sharing intentions. In the control, participants were equally like to say they would share false versus true headlines at COVID-19 whereas, in the treatment, sharing of true headlines was significantly higher than false headlines. Our results ‚Äì which mirror those found previously for political fake news ‚Äì suggest that nudging people to think about accuracy is a simple way to improve choices about what to share on social media. Accuracy nudges are straightforward for social media platforms to implement on top of the other approaches they are currently employing, and could have an immediate positive impact on stemming the tide of misinformation about the COVID-19 outbreak.},
  langid = {english},
  keywords = {m√©dias sociaux,nudge},
  annotation = {ZSCC: 0000087  http://web.archive.org/web/20200509222239/https://psyarxiv.com/uhbk9/},
  file = {D\:\\Home\\dbelveze\\OneDrive - Universit√© de Rennes 1\\Zotero\\PsyArXiv2020\\Pennycook_McPhetres & al._2020_Fighting COVID-19 misinformation on social media_.pdf}
}

@article{PennycookImpliedTruthEffect2020,
  title = {The {{Implied Truth Effect}}: {{Attaching Warnings}} to a {{Subset}} of {{Fake News Headlines Increases Perceived Accuracy}} of {{Headlines Without Warnings}}},
  shorttitle = {The {{Implied Truth Effect}}},
  author = {Pennycook, Gordon and Bear, Adam and Collins, Evan T. and Rand, David G.},
  date = {2020-02-21},
  journaltitle = {Management Science},
  shortjournal = {Management Science},
  volume = {66},
  number = {11},
  pages = {4944--4957},
  publisher = {{INFORMS}},
  issn = {0025-1909},
  doi = {10.1287/mnsc.2019.3478},
  url = {https://pubsonline.informs.org/doi/10.1287/mnsc.2019.3478},
  urldate = {2021-02-03},
  abstract = {What can be done to combat political misinformation? One prominent intervention involves attaching warnings to headlines of news stories that have been disputed by third-party fact-checkers. Here we demonstrate a hitherto unappreciated potential consequence of such a warning: an implied truth effect, whereby false headlines that fail to get tagged are considered validated and thus are seen as more accurate. With a formal model, we demonstrate that Bayesian belief updating can lead to such an implied truth effect. In Study 1 (n = 5,271 MTurkers), we find that although warnings do lead to a modest reduction in perceived accuracy of false headlines relative to a control condition (particularly for politically concordant headlines), we also observed the hypothesized implied truth effect: the presence of warnings caused untagged headlines to be seen as more accurate than in the control. In Study 2 (n = 1,568 MTurkers), we find the same effects in the context of decisions about which headlines to consider sharing on social media. We also find that attaching verifications to some true headlines‚Äîwhich removes the ambiguity about whether untagged headlines have not been checked or have been verified‚Äîeliminates, and in fact slightly reverses, the implied truth effect. Together these results contest theories of motivated reasoning while identifying a potential challenge for the policy of using warning tags to fight misinformation‚Äîa challenge that is particularly concerning given that it is much easier to produce misinformation than it is to debunk it.This paper was accepted by Elke Weber, judgment and decision making.},
  file = {D\:\\Home\\dbelveze\\OneDrive - Universit√© de Rennes 1\\Zotero\\Management Science2020\\Pennycook_Bear & al._2020_The Implied Truth Effect_.pdf;D\:\\Home\\dbelveze\\OneDrive - Universit√© de Rennes 1\\Zotero\\storage\\ALGNAKM5\\mnsc.2019.html}
}

