@article{BatesMappingdynamictranscriptome2019,
  ids = {BatesMappingdynamictranscriptome2019a},
  title = {Mapping of Dynamic Transcriptome Changes Associated with Silica-Triggered Autoimmune Pathogenesis in the Lupus-Prone {{NZBWF1}} Mouse},
  author = {Bates, M.A. and Benninghoff, A.D. and Gilley, K.N. and Holian, A. and Harkema, J.R. and Pestka, J.J.},
  date = {2019},
  journaltitle = {Frontiers in Immunology},
  shortjournal = {Front. Immunol.},
  volume = {10},
  issn = {1664-3224},
  doi = {10.3389/fimmu.2019.00632},
  url = {https://www.embase.com/search/results?subaction=viewrecord&id=L627957886&from=export},
  abstract = {Crystalline silica (cSiO2) is a widely recognized environmental trigger of autoimmune disease. In the lupus-prone female NZBWF1 mouse, airway exposure to cSiO2 triggers pulmonary ectopic lymphoid neogenesis, systemic autoantibody elevation, and glomerulonephritis. Here we tested the hypothesis that upregulation of adaptive immune function genes in the lung precedes cSiO2-triggering of autoimmune disease in this model. The study include three groups of mice, as follows: (1) necropsied 1 d after a single intranasal instillation of 1 mg cSiO2 or vehicle, (2) necropsied 1 d after four weekly single instillations of 1 mg cSiO2 or vehicle, or (3) necropsied 1, 5, 9, or 13 weeks after four weekly single instillations of 1 mg cSiO2 or vehicle. NanoString nCounter analysis revealed modest transcriptional changes associated with innate and adaptive immune response as early as 1 d after a single cSiO2 instillation. These responses were greatly expanded after four weekly cSiO2 instillations. Concurrent with ectopic lymphoid neogenesis, dramatic increases in mRNAs associated with chemokine release, cytokine production, sustained interferon activity, complement activation, and adhesion molecules were observed. As disease progressed, expression of these genes persisted and was further amplified. Consistent with autoimmune pathogenesis, the time between 5 and 9 weeks post-instillation reflected an important transition period where considerable immune gene upregulation in the lung was observed. Upon termination of the chronic study (13 weeks), cSiO2-induced changes in transcriptome signatures were similarly robust in kidney as compared to the lung, but more modest in spleen. Transcriptomic signatures in lung and kidney were indicative of infiltration and/or expansion of neutrophils, macrophages, dendritic cells, B cells, and T cells that corresponded with accelerated autoimmune pathogenesis. Taken together, airway exposure to cSiO2 elicited aberrant mRNA signatures for both innate and adaptive immunity that were consistent with establishment of the lung as the central autoimmune nexus for launching systemic autoimmunity and ultimately, kidney injury.},
  issue = {MAR},
  langid = {english},
  keywords = {adaptive immunity,alternative complement pathway C3 C5 convertase,animal experiment,animal model,animal tissue,article,autoimmunity,autopsy,B lymphocyte,CD14 antigen,CD8+ T lymphocyte,CD83 antigen,cell adhesion molecule,cell population,complement activation,complement c3a receptor 1,complement factor D,controlled study,correlation analysis,CXCL1 chemokine,CXCL13 chemokine,cyclic AMP,cytokine release,cytotoxic T lymphocyte antigen 4,dendritic cell,disease exacerbation,epithelial derived neutrophil activating factor 78,exposure,female,gamma interferon inducible protein 10,gelatinase B,gene expression,gene expression level,gene function,genetic analyzer,genetic transcription,hamp protein,immune response,infection sensitivity,innate immunity,interferon,interferon induced protein with tetratricopeptide repeats 3,interferon regulatory factor 7,interleukin 11 alpha receptor 1,lupus vulgaris,lymphoid tissue,macrophage,macrophage inflammatory protein 1alpha,macrophage receptor,molecular dynamics,monocyte chemotactic protein 1,monocyte chemotactic protein 3,mouse,natural killer cell,natural resistance associated macrophage protein 1,nCounter,neutrophil,nonhuman,osteopontin,peptides and proteins,protein tyrosine kinase,radical s adenosyl methionine domain containing protein 2,regulatory T lymphocyte,reverse transcription polymerase chain reaction,RNA sequence,silicon dioxide,transcriptome,unclassified drug,unindexed drug,upregulation},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Frontiers in Immunology2019\\Bates_Benninghoff & al._2019_Mapping of dynamic transcriptome changes associated with silica-triggered_.pdf;D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Frontiers in Immunology2019\\Bates_Benninghoff & al._2019_Mapping of dynamic transcriptome changes associated with silica-triggered_2.pdf}
