  title = {Cutaneous loxoscelism, about an exceptional observation of 9 consecutive cases},
  author = {Boissiere, F. and Masson, R. and Fluieraru, S. and Vitse, J. and Dessena, L. and Lefevre, M. and Bekara, F. and Herlin, C.},
  date = {2016-12},
  journaltitle = {Annales De Chirurgie Plastique Esthetique},
