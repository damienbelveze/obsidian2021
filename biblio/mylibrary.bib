@article{KahanScienceCuriosityPolitical2017,
  title = {Science {{Curiosity}} and {{Political Information Processing}}},
  author = {Kahan, Dan M. and Landrum, Asheley and Carpenter, Katie and Helft, Laura and Jamieson, Kathleen Hall},
  date = {2017},
