@report{deMeloAntiCOVID19efficacyivermectin2020,
  type = {preprint},
  ids = {deMeloAntiCOVID19efficacyivermectin2020a},
  title = {Anti-{{COVID}}-19 Efficacy of Ivermectin in the Golden Hamster},
  author = {de Melo, Guilherme Dias and Lazarini, Françoise and Larrous, Florence and Feige, Lena and Kergoat, Lauriane and Marchio, Agnès and Pineau, Pascal and Lecuit, Marc and Lledo, Pierre-Marie and Changeux, Jean-Pierre and Bourhy, Hervé},
  options = {useprefix=true},
  date = {2020-11-22},
  institution = {{Immunology}},
  doi = {10.1101/2020.11.21.392639},
  url = {http://biorxiv.org/lookup/doi/10.1101/2020.11.21.392639},
  urldate = {2021-09-03},
  abstract = {The devastating coronavirus disease 2019 (COVID-19) pandemic, due to SARS-CoV-2, has caused more than 47 million confirmed cases and more than 1.2 million human deaths around the globe             1             , and most of the severe cases of COVID-19 in humans are associated with neurological symptoms such as anosmia and ageusia, and uncontrolled inflammatory immune response             2–5             . Among therapeutic options             6–8             , the use of the anti-parasitic drug ivermectin (IVM), has been proposed, given its possible anti-SARS-CoV-2 activity             9             . Ivermectin is a positive allosteric modulator of the α-7 nicotinic acetylcholine receptor             10             , which has been suggested to represent a target for the control of Covid-19 infection             11             , with a potential immunomodulatory activity             12             . We assessed the effects of IVM in SARS-CoV-2-intranasally-inoculated golden Syrian hamsters. Even though ivermectin had no effect on viral load, SARS-Cov-2-associated pathology was greatly attenuated. IVM had a sex-dependent and compartmentalized immunomodulatory effect, preventing clinical deterioration and reducing olfactory deficit in infected animals. Importantly, ivermectin dramatically reduced the             Il-6/Il-10             ratio in lung tissue, which likely accounts for the more favorable clinical presentation in treated animals. Our data support IVM as a promising anti-COVID-19 drug candidate.},
  langid = {english}
}

