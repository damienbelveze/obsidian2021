  author = {de publication Kintz, Salomé Directeur and Bats, Raphaëlle and Belvèze, Damien and Defosse, Marie-Françoise and Defrance, Agnès and Frau-Meigs, Divina and Froissart, Pascal and Glass, Valérie and Hyvernat-Duchêne, Anne-Cécile and Jehel, Sophie and Las Vergnas, Olivier and Pascal, Sophie and Ritte, Marie-Anne and Robin, Valérie and Swiatek, Cécile},
