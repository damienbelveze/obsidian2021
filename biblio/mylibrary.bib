  pmcid = {PMC5396866},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\PloS One2017\\Robinson_Kennedy & al._2017_Defining the complex phenotype of severe systemic loxoscelism using a large_.pdf}
}

